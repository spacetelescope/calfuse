export CF_DIR="@PREFIX@"
export PATH="${CF_DIR}/bin:${PATH}"

export CF_DATADIR="${CF_DIR}/share/@PACKAGE@-@VERSION@"
export CF_CALDIR="${CF_DATADIR}/calfiles"
export CF_PARMDIR="${CF_DATADIR}/parmfiles"
export CF_IDLDIR="${CF_DATADIR}/idl"
export CF_HISTDIR=`pwd`

